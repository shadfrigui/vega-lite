  "vconcat": [
    {
      "title": {
        "text": ["Warren Buffett's Top 5 Holdings, 2012–2022"],
        "fontSize": 44,
        "subtitle": [
          "Although old-guard favorites such as Coca-Cola and American Express still form the core of the portfolio,",
          "Buffett's Berkshire Hathaway has taken a shine to names such as Apple and Bank of America. Hover over",
          "each point for the value of the holdings and the percentage of Berkshire’s total portfolio."
        ],
        "subtitleFontSize": 19,
        "subtitlePadding": 10,
        "subtitleLineHeight": 25,
        "dx": -65
      },
      "mark": "text"
    },
    {
      "width": 700,
      "height": 350,
      "transform": [
        {
          "joinaggregate": [{"op": "sum", "field": "Holdings", "as": "Sum"}],
          "groupby": ["Year"]
        },
        {"calculate": "datum.Holdings / datum.Sum", "as": "Percentage"},
        {"filter": "datum.Company != 'Others' && datum.Year >= 2012"},
        {
          "window": [{"op": "rank", "as": "Rank"}],
          "groupby": ["Year"],
          "sort": [{"field": "Holdings", "order": "descending"}]
        },
        {"filter": "datum.Rank <= 5"},
        {
          "calculate": "datum.Company == 'American Express' ?['American','Express']: datum.Company == 'Bank of America' ?['Bank','of America'] : datum.Company",
          "as": "YLabels"
        },
        {
          "calculate": "replace(format(datum.Holdings, '.3s'), 'G', 'B')",
          "as": "HoldingsTooltip"
        }
      ],
      "encoding": {
        "x": {"field": "Year", "type": "ordinal"},
        "y": {"field": "Rank", "type": "ordinal", "axis": null},
        "color": {
          "field": "Company",
          "scale": {"scheme": "tableau20"},
          "legend": null
        },
        "tooltip": [
          {"field": "Year"},
          {"field": "Company"},
          {"field": "Rank"},
          {"field": "HoldingsTooltip", "title": "Holdings"},
          {"field": "Percentage", "format": ".1%"}
        ]
      },
      "layer": [
        {"mark": {"type": "line"}},
        {
          "params": [
            {
              "name": "highlight",
              "select": {"type": "point", "on": "mouseover"}
            }
          ],
          "mark": {"type": "point"},
          "encoding": {
            "stroke": {
              "condition": [
                {"param": "highlight", "empty": false, "value": "#000000"}
              ],
              "value": "#FFFFFF"
            }
          }
        },
        {
          "transform": [{"filter": {"field": "Year", "oneOf": [2012, 2022]}}],
          "mark": {"type": "text", "fontSize": 15},
          "encoding": {"text": {"field": "Rank"}, "color": {"value": "white"}}
        },
        {
          "transform": [{"filter": {"field": "Year", "oneOf": [2012, 2022]}}],
          "mark": {
            "type": "text",
            "fontSize": 17,
            "xOffset": {"expr": "datum.Year == 2012 ? -55 : 55"},
            "baseline": {
              "expr": "datum.Company == 'American Express' || datum.Company == 'Bank of America' ? 'bottom':'middle'"
            }
          },
          "encoding": {
            "text": {"field": "YLabels"},
            "color": {"value": "black"}
          }
        },
        {
          "transform": [
            {"filter": {"field": "Year", "oneOf": [2013, 2015, 2020, 2021]}},
            {"filter": "datum.Rank == 5"}
          ],
          "mark": {
            "type": "text",
            "fontSize": 17,
            "yOffset": -20,
            "xOffset": {
              "expr": "datum.Year == 2020 ? -15 : datum.Year == 2021 ? 15 : 0 "
            }
          },
          "encoding": {
            "text": {"field": "Company"},
            "color": {"value": "black"}
          }
        }
      ]
